
<div class="search-container">
  <form action="<%= searchPath %>" method="post">
    <input type="text" placeholder="Buscar..." name="searchCat">
    <button type="submit" class="searchButton" value="Submit">
      <i class="fa fa-search"></i>
    </button>
  </form>
</div>
<% if (!objectsList.length) { %>
  <p>No results</p>
<% } else { %>


  <% objectsList.forEach((object) => { %>

  <row>
  <column>
    <info>
      <img src="http://placehold.it/150x150">
    </info>
  </column>
  <column>
    <info>
      <div class="name"> <%= object.name %> </div>
      <div class="text"> <%= object.description %> </div>
      <div class="text"> <%= object.status %> </div>
    </info>
  </column>
  <column>
    <info>
      <div>
        <a href="<%= showObjectPath(object) %>">Ver</a>
      </div>
      <!-- <% if (currentUser) { %>
        <div>
          <a href="<%= editObjectPath(object) %>">Editar</a>
        </div>
        <div>
          <form action="<%= deleteObjectPath(object) %>" method="POST">
            <input type="hidden" name="_method" value="delete" />
            <input type="submit" value="Delete" />
          </form>
        </div>
      <% } %> -->
    </info>
  </column>
</row>
  <% }) %>
<% } %>
